  "dependencies": {
    "@kuscamara/code-sample": "^3.0.0",
    "@polymer/app-layout": "^3.0.0-pre.23",
    "@polymer/app-route": "^3.0.0-pre.23",
    "@polymer/iron-icons": "^3.0.0-pre.21",
    "@polymer/iron-pages": "^3.0.0-pre.23",
    "@polymer/paper-button": "^3.0.0-pre.21",
    "@polymer/paper-checkbox": "^3.0.0-pre.21",
    "@polymer/paper-dialog": "^3.0.0-pre.21",
    "@polymer/paper-dialog-scrollable": "^3.0.0-pre.21",
    "@polymer/paper-input": "^3.0.0-pre.21",
    "@polymer/paper-progress": "^3.0.0-pre.21",
    "@polymer/paper-spinner": "^3.0.0-pre.21",
    "@polymer/polymer": "^3.0.5",
    "@webcomponents/webcomponentsjs": "^2.1.0",
    "debounce": "^1.2.0",
    "highlight.js": "^9.12.0",
    "long": "^4.0.0",
    "protobufjs": "^6.8.8"
  },
  "scripts": {
    "lint": "make lint",
    "test": "yarn lint && yarn build",
    "start": "webpack-dev-server --open --config ./webpack.dev.conf.js",
    "build": "yarn protobuf && webpack --config ./webpack.prod.conf.js",
    "protobuf": "pbjs --force-long -t static-module -w es6 -o src/feedlightpb.js -p /usr/include/ ../srv/feedlightpb/feedlight.proto google/protobuf/any.proto google/api/annotations.proto && pbts -o src/feedlightpb.d.ts src/feedlightpb.js"
  },
  "standardx": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "window",
      "customElements",
      "config",
      "fetch"
    ]
  },
  "eslintConfig": {
    "rules": {
      "no-undef": "off"
    }
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "eslint-plugin-typescript": "^0.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "path-override-webpack-plugin": "^0.1.2",
    "standardx": "^2.1.0",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
